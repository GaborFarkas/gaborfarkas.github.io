function custom() {
    var eov = new L.Proj.CRS('EPSG:23700', '+proj=somerc +lat_0=47.14439372222222 +lon_0=19.04857177777778 +k_0=0.99993 +x_0=650000 +y_0=200000 +ellps=GRS67 +towgs84=52.17,-71.82,-14.9,0,0,0,0 +units=m +no_defs',
        {
            resolutions: [8192, 4096, 2048, 1024, 512, 256, 128, 64, 32, 16, 8, 4]
        }
    );
    map = new L.mapbox.Map('map', null, {
        layers: [
            L.tileLayer.wms("http://www.agt.bme.hu/cgi-bin/mapserv?map=/var/www/html/gis/wms/eu_dem/eu_dem.map", {
            layers: 'mo_eov_szines',
            format: 'image/png',
            transparent: true,
            })
        ],
        center: [47,20],
        zoom: 3,
        crs: eov
    });
}