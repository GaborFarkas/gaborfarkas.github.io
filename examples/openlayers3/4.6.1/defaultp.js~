function defaultp() {
    map = new ol.Map({
        target: 'map',
        layers: [
            new ol.layer.Vector({
                    source: new ol.source.GeoJSON({
                        url: '../../../res/vector/countries_trunk.geojson'
                    })
                })
        ],
        view: new ol.View({
            projection: 'EPSG:4326',
            center: [24,45],
            zoom: 4
        })
    });
}