function attribute() {
    var featArray = [];
    var layer = map.layers[0];
    var filter = new OpenLayers.Filter.Comparison({
        type: '==',
        property: 'name',
        value: 'Hungary'
    });
    for (var i=0;i<layer.features.length;i++) {
        var feat = layer.features[i];
        if (filter.evaluate(feat)) {
            featArray.push(feat);
        }
    }
    return featArray;
}