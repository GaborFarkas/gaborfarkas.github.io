function filterlyr() {
    function filterFeature(feature, property, value, operator) {
        var bool;
        if (feature.properties[property]) {
            bool = false;
            var featProp = feature.properties[property];
            switch (operator) {
                case "==":
                    bool = (featProp == value);
                    break;
                case "<":
                    bool = (featProp < value);
                    break;
                case "<=":
                    bool = (featProp <= value);
                    break;
                case ">":
                    bool = (featProp > value);
                    break;
                case ">=":
                    bool = (featProp >= value);
                    break;
                case "!=":
                    bool = (featProp != value);
                    break;
                default:
                    throw new TypeError("Operator not supported. Valid operators: '==', '!=' '<', '<=', '>', '>='.");
                    break;
            }
        }
        return bool;
    }
    map.featureLayer.setFilter(function(feat) {
        return filterFeature(feat, 'name', 'Hungary', '==');
    });
}